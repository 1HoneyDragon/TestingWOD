{
  "private": true,
  "name": "tgui-workspace",
  "version": "4.3.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "tgui:build": "webpack",
    "tgui:analyze": "webpack --analyze",
    "tgui:dev": "node --experimental-modules packages/tgui-dev-server/index.js",
    "tgui:lint": "eslint packages --ext .js,.cjs,.ts,.tsx",
    "tgui:sonar": "eslint packages --ext .js,.cjs,.ts,.tsx -c .eslintrc-sonar.yml",
    "tgui:tsc": "tsc",
    "tgui:test": "jest --watch",
    "tgui:test-simple": "CI=true jest --color",
    "tgui:test-ci": "CI=true jest --color --collect-coverage"
  },
  "dependencies": {
    "@babel/core": "^7.13.14",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-transform-jscript": "^7.12.13",
    "@babel/preset-env": "^7.13.12",
    "@types/jest": "^26.0.22",
    "@types/jsdom": "^16.2.9",
    "@types/node": "^14.14.37",
    "@typescript-eslint/parser": "^4.20.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-inferno": "^6.1.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "common": "workspace:*",
    "css-loader": "^5.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.23.0",
    "eslint-plugin-react": "^7.23.1",
    "file-loader": "^6.2.0",
    "inferno": "^7.4.7",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "jsdom": "^16.5.2",
    "mini-css-extract-plugin": "^1.4.0",
    "sass": "^1.32.6",
    "sass-loader": "^11.0.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.28.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.6.0"
  }
}
